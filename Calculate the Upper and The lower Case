def string_test(s):
    d={"UPPER_CASE":0, "LOWER_CASE":0}
    for c in s:
        if c.isupper():
           d["UPPER_CASE"]+=1
        elif c.islower():
           d["LOWER_CASE"]+=1        
    print ("Original String : ", s)
    print ("count for Upper case characters : ", d["UPPER_CASE"])
    print ("count for Lower case Characters : ", d["LOWER_CASE"])
string_test('Prashant Pandey Is gooD')
